<Application x:Class="timeline_test.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:timeline_test"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- Global scaling factor for all windows - change this value to scale all forms -->
        <ScaleTransform x:Key="GlobalWindowScaleTransform" ScaleX="1.5" ScaleY="1.5" />
    </Application.Resources>
</Application>
